{
   "database": {
      "rules": "database.rules.json"
   },
   "hosting": {
      "rewrites": [
         {
            "source": "**/!(*.css|*.js|*.map|*.jpg|*.gif|*.png|*.php)",
            "destination": "/index.html"
         }
      ],
      "public": "./dist",
      "ignore": ["firebase.json", "**/.*", "**/node_modules/**"]
   }
}
